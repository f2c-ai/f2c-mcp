{
  "name": "@f2c/mcp",
  "version": "0.4.9",
  "description": "",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f2c-ai/f2c-mcp",
    "directory": "."
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": ["f2c", "mcp"],
  "files": ["dist"],
  "main": "dist/stdio.js",
  "types": "dist/stdio.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/stdio.d.mts",
        "default": "./dist/stdio.mjs"
      },
      "require": {
        "types": "./dist/stdio.d.ts",
        "default": "./dist/stdio.js"
      }
    },
    "./http": {
      "import": {
        "types": "./dist/http.d.mts",
        "default": "./dist/http.mjs"
      },
      "require": {
        "types": "./dist/http.d.ts",
        "default": "./dist/http.js"
      }
    }
  },
  "bin": {
    "f2c-mcp": "dist/cli.js"
  },
  "scripts": {
    "dev": "bun --watch run src/http.ts",
    "serve": "bun run src/http.ts",
    "business": "bun run scripts/start-business-processor.ts",
    "test": "bun run scripts/auto-test.ts",
    "build:prod": "bun run build.ts",
    "build:dev": "bun --watch run build.ts",
    "inspector": "npx @modelcontextprotocol/inspector node ./dist/stdio.js",
    "lint": "biome check . --fix"
  },
  "author": "ckken",
  "maintainers": ["ckken"],
  "license": "ISC",
  "dependencies": {
    "@elysiajs/static": "^1.4.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "elysia": "^1.4.13",
    "fetch-to-node": "^2.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.2",
    "@empjs/biome-config": "2.3.0",
    "@modelcontextprotocol/sdk": "1.20.2",
    "@types/bun": "^1.2.13",
    "@types/ws": "^8.18.1",
    "typescript": "^5.9.3",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
